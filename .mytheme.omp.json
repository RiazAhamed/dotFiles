{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "<#606770>\u256d<#606770>\u2500 <#84ba64>{{ .HostName }}</><#606770> | ",
          "type": "session"
        },
        {
          "foreground": "#ED5144",
          "style": "plain",
          "template": "</>{{ .Path }}</>",
          "type": "path"
        },
        {
          "foreground": "#FF9248",
          "style": "plain",
          "template": "<#606770>\u2500(</>#<#606770> | </>",
          "type": "root"
        },
        {
          "foreground": "#e0f8ff",
          "properties": {
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": false
          },
          "style": "plain",
          "template": "<#606770> | </><#ffe193>\uEA68<#606770></> {{ .HEAD }}{{ if .Staging.Changed }}<#84ba64> {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#ED5144> {{ .Working.String }}</>{{ end }}<#606770></><#606770>",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#84ba64",
          "properties": {
            "fetch_package_manager": true
          },
          "style": "plain",
          "template": "<#84ba64>Node@v{{ .Full }}</><#606770> | </>",
          "type": "node"
        },
        {
          "foreground": "#FFE873",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "python"
        },
        {
          "foreground": "#ec2729",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "java"
        },
        {
          "foreground": "#0d6da8",
          "style": "plain",
          "template": "<#606770></>{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "dotnet"
        },
        {
          "foreground": "#06aad5",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "go"
        },
        {
          "foreground": "#925837",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "rust"
        },
        {
          "foreground": "#055b9c",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "dart"
        },
        {
          "foreground": "#ED5144",
          "style": "plain",
          "template": "<#ED5144>ng@v{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "angular"
        },
        {
          "foreground": "#359a25",
          "style": "plain",
          "template": "<#606770></>julia->{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "julia"
        },
        {
          "foreground": "#9c1006",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "ruby"
        },
        {
          "foreground": "#5398c2",
          "style": "plain",
          "template": "<#606770></>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#606770> | </>",
          "type": "azfunc"
        },
        {
          "foreground": "#faa029",
          "style": "plain",
          "template": "<#606770></>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#606770> | </>",
          "type": "aws"
        },
        {
          "foreground": "#316ce4",
          "style": "plain",
          "template": "<#606770></>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#606770> | </>",
          "type": "kubectl"
        },
        {
          "foreground": "#606770",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FFFB38{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "style": "plain",
          "template": "<#84ba64>Git-{{ if not .Error }}{{ .Percentage }}{{ end }}{{ .Error }}%</>",
          "type": "battery"
        },
        {
          "foreground": "#316dca",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "<#606770> | </>{{ .CurrentDate | date .Format }}</>",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "<#606770>\u2570<#606770>\u2500",
          "type": "text"
        },
        {
          "foreground": "#606770",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " ðŸ”¥ ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "osc99": true,
  "version": 2
}
